@model Bands.WEB.Models.ViewModels.DemoViewModel
<div id="create-demo">
    <div class="form-group no-margin">
        <div class="input-group mb-2">
            <div class="input-group-prepend">
                <div class="input-group-text label-profile-edit">
                    <label asp-for="Name" class="control-label">Name</label>
                </div>
            </div>
            <input asp-for="Name" class="form-control" />
        </div>
        <span asp-validation-for="Name" class="text-danger"></span>
    </div>
    <div class="form-group no-margin">
        <div class="input-group mb-2">
            <div class="input-group-prepend">
                <div class="input-group-text label-profile-edit">
                    <label asp-for="Link" class="control-label">Link</label>
                </div>
            </div>
            <input asp-for="Link" class="form-control" />
        </div>
        <span asp-validation-for="Link" class="text-danger"></span>
    </div>
    <div class="text-center">
        <input type="submit" id="demo-submit" class="btn btn-default" value="Save">
    </div>
</div>
<div id="demo-callback-message"></div>
